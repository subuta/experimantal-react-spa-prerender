{
  "name": "example",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production parcel-u build src/index.html",
    "start:dev": "parcel-u serve src/index.html",
    "export:dev": "parcel-u export src/index.html -u /foo -u /bar -u /nested/baz",
    "export:prod": "NODE_ENV=production parcel-u export src/index.html -u /foo -u /bar -u /nested/baz",
    "start:prod": "NODE_ENV=production parcel-u serve src/index.html",
    "static": "cd ./dist/client && serve -l 4000",
    "start": "run-s start:prod",
    "dev": "run-s start:dev",
    "spa": "parcel ./src/index.html"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.2",
    "tailwindcss": "^1.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@hot-loader/react-dom": "^16.9.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "parcel-u": "^0.9.2",
    "parcel-u-react": "^0.9.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.1",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.14",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "recompose": "^0.30.0",
    "serve": "^11.2.0"
  }
}
