{
  "name": "example",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "build": "run-p build:*",
    "build:client": "parcel build ./src/client.js",
    "build:server": "parcel build --target node src/server.js",
    "server": "run-s build:server start:server",
    "watch:client": "parcel watch ./src/client.js",
    "dev": "run-p watch:client server"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "parcel-bundler": "^1.12.3",
    "postcss-nested": "^4.1.2",
    "postcss-modules": "^1.4.1",
    "tailwindcss": "^1.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@hot-loader/react-dom": "^16.9.0",
    "async-exit-hook": "^2.0.1",
    "common-tags": "^1.8.0",
    "koa": "^2.8.2",
    "koa-body": "^4.1.1",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "loud-rejection": "^2.2.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.14"
  }
}
