{
  "name": "parcel-u",
  "version": "0.9.3",
  "description": "Parcel-based universal renderer for SPA(Single-page Application) project.",
  "main": "src/index.js",
  "files": [
    "bin/**/*",
    "lib/**/*",
    "src/**/*",
    "index.js"
  ],
  "bin": {
    "parcel-u": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:subuta/parcel-u.git"
  },
  "keywords": [
    "parcel",
    "universal",
    "spa",
    "web",
    "ssr"
  ],
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/parcel-u/issues"
  },
  "homepage": "https://github.com/subuta/parcel-u#readme",
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "arg": "^4.1.1",
    "async-exit-hook": "^2.0.1",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "common-tags": "^1.8.0",
    "esm": "^3.2.25",
    "find-up": "^4.1.0",
    "koa": "^2.8.2",
    "koa-body": "^4.1.1",
    "koa-compose": "^4.1.0",
    "koa-connect": "^2.0.1",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "clear-module": "^4.0.0",
    "lodash": "^4.17.15",
    "loud-rejection": "^2.2.0",
    "mkdirp": "^0.5.1",
    "parcel-bundler": "^1.12.3",
    "resolve": "^1.12.0"
  }
}
