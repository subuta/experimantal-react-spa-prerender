{
  "name": "parcel-u",
  "version": "1.0.0",
  "description": "Parcel-based universal renderer for SPA(Single-page Application) project.",
  "main": "index.js",
  "bin": {
    "parcel-u": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/experimantal-react-spa-prerender.git"
  },
  "keywords": [
    "parcel",
    "universal",
    "spa",
    "web",
    "ssr"
  ],
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/experimantal-react-spa-prerender/issues"
  },
  "homepage": "https://github.com/subuta/experimantal-react-spa-prerender#readme",
  "alias": {
    "react-dom": "@hot-loader/react-dom"
  },
  "scripts": {
    "build": "NODE_ENV=production node -r esm ./bin/bundle.js",
    "start:dev": "node -r esm ./bin/server.js ../../example/react/src/index.html",
    "export:dev": "node -r esm ./bin/export.js",
    "export:prod": "NODE_ENV=production node -r esm ./bin/export.js",
    "start:prod": "NODE_ENV=production node -r esm ./bin/server.js",
    "start": "run-s start:prod",
    "dev": "run-s start:dev"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "clear-module": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.2",
    "tailwindcss": "^1.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@hot-loader/react-dom": "^16.9.0",
    "arg": "^4.1.1",
    "async-exit-hook": "^2.0.1",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "common-tags": "^1.8.0",
    "esm": "^3.2.25",
    "find-up": "^4.1.0",
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.8.2",
    "koa-body": "^4.1.1",
    "koa-compose": "^4.1.0",
    "koa-connect": "^2.0.1",
    "koa-logger": "^3.2.1",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "loud-rejection": "^2.2.0",
    "mkdirp": "^0.5.1",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.14",
    "react-is": "^16.10.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-ssr-prepass": "^1.0.7",
    "recompose": "^0.30.0"
  },
  "peerDependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  }
}
